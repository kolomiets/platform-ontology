
@prefix : <http://www.semanticweb.org/kolomiets/ontologies/0.0.1/cdm#> .
@prefix cdm: <http://www.semanticweb.org/kolomiets/ontologies/0.0.1/cdm#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.semanticweb.org/kolomiets/ontologies/0.0.1/cdm>
  a owl:Ontology ;
  rdfs:label "Common Data Model" ;
  rdfs:comment "Common Data Model ontology." ;
  .

cdm:Project a owl:Class ;
  rdfs:label "Project" ;
  rdfs:comment "A simple project entity." .

cdm:Component a owl:Class ;
  rdfs:label "Component" ;
  rdfs:comment "A component used by a project." .

cdm:usesComponent a owl:ObjectProperty ;
  rdfs:domain cdm:Project ;
  rdfs:range cdm:Component ;
  rdfs:label "uses component" .

cdm:name a owl:DatatypeProperty ;
  rdfs:domain cdm:Project ;
  rdfs:range xsd:string ;
  rdfs:label "name" .
